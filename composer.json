{
    "name": "t3docs/blog-example",
    "authors": [
        {
            "name": "TYPO3 Core Team",
            "role": "Developer"
        }
    ],
    "type": "typo3-cms-extension",
    "description": "This extension contains code examples used in TYPO3 explained to describe the use of Extbase",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "T3docs\\BlogExample\\": "Classes/"
        }
    },
    "require": {
        "typo3/cms-core": "^10.4"
    },
    "replace": {
        "friendsoftypo3/blog-example": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.4.x-dev"
        },
        "typo3/cms": {
            "extension-key": "blog_example",
            "web-dir": ".Build/web"
        }
    },
    "scripts": {
        "prepare-release": [
            "rm -rf .github",
            "rm .gitignore",
            "rm .editorconfig"
        ]
    }
}
