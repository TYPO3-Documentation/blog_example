{
  "name": "friendsoftypo3/blog-example",
  "description": "This blog example extension is described in the TYPO3 documentation. - Developing TYPO3 Extensions with Extbase and Fluid.",
  "license": "GPL-2.0-or-later",
  "type": "typo3-cms-extension",
  "authors": [
    {
      "name": "TYPO3 Core Team",
      "role": "Developer"
    }
  ],
  "homepage": "https://extensions.typo3.org/extension/blog_example/",
  "support": {
    "issues": "https://github.com/FriendsOfTYPO3/blog_example/issues",
    "source": "https://github.com/FriendsOfTYPO3/blog_example"
  },
  "require": {
    "typo3/cms-core": "^12.0.0 || dev-main",
    "typo3/cms-extbase": "^12.0.0 || dev-main",
    "typo3/cms-fluid": "^12.0.0 || dev-main"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.28",
    "typo3/coding-standards": "^0.5.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FriendsOfTYPO3\\BlogExample\\": "Classes/"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true,
      "ergebnis/composer-normalize": true
    },
    "bin-dir": ".Build/bin",
    "sort-packages": true,
    "vendor-dir": ".Build/vendor"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "12.0.x-dev"
    },
    "typo3/cms": {
      "extension-key": "blog_example",
      "web-dir": ".Build/web"
    }
  },
  "scripts": {
    "prepare-release": [
      "rm -rf .github",
      "rm .gitignore",
      "rm .editorconfig"
    ]
  }
}
